{
  "name": "swagger-generate-ts",
  "displayName": "swagger-generate-ts",
  "description": "Swagger api 文档生成Interface",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "repository": {
    "url": "https://github.com/leizelong/swagger-generate-ts"
  },
  "publisher": "leizl",
  "license": "",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:swagger-generate-ts.definitions"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "swagger-generate-ts.definitions",
        "title": "SwaggerApi生成Ts声明和请求",
        "category": "SwaggerGenerateTs"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "设置openApiJson获取Url",
      "properties": {
        "swagger-generate-ts.openApiJsonUrlOptions": {
          "type": "array",
          "description": "配置多份openApiJsonUrl"
        },
        "swagger-generate-ts.requestImportPath": {
          "type": "string",
          "default": "@/utils/request",
          "description": "import { get, post } from '${requestImportPath}'"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/types": "^7.20.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "ast-types": "^0.14.2",
    "babylon": "^6.18.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "html-loader": "^4.2.0",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@babel/parser": "^7.20.1",
    "axios": "^1.1.3",
    "lodash": "^4.17.21",
    "openapi-typescript": "^5.4.1",
    "recast": "^0.21.5"
  }
}
